    },
    {
      "line_order": 3,
      "speaker": "npc",
      "text_ja": "お気をつけてお帰りください。",
      "text_ja_male": null,
      "text_ja_female": null,
      "pronunciation_ko": "오키오츠케테 오카에리쿠다사이",
      "text_ko": "조심해서 돌아가세요.",
      "grammar_hint": "お気をつけて는 '조심해서'라는 배려 표현입니다.",
      "key_expressions": [
        "お気をつけて",
        "お帰りください"
      ],
      "furigana": [
        {
          "text": "お"
        },
        {
          "text": "気",
          "reading": "き"
        },
        {
          "text": "をつけてお"
        },
        {
