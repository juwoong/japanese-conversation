    },
    {
      "line_order": 2,
      "speaker": "user",
      "text_ja": "一人です。メニューをお願いします。",
      "text_ja_male": null,
      "text_ja_female": null,
      "pronunciation_ko": "히토리데스. 메뉴-오 오네가이시마스",
